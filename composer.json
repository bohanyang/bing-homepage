{
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Brent, Bohan Yang",
            "email": "brent@manyou.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^3.0",
        "async-aws/s3": "^1.12",
        "chubbyphp/chubbyphp-workerman-request-handler": "^2.0",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^3.5.1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/migrations": "^3.2",
        "guzzlehttp/promises": "^1.4",
        "league/uri": "^6.8",
        "league/uri-components": "^2.4",
        "monolog/monolog": "^3.0",
        "nyholm/psr7": "^1.5",
        "pentatrion/vite-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.11",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/log": "^2.0 || ^3.0",
        "rompetomp/inertia-bundle": "^2.4",
        "symfony/config": "^6.1",
        "symfony/console": "6.2.*",
        "symfony/dependency-injection": "^6.1",
        "symfony/doctrine-messenger": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^6.1",
        "symfony/http-client": "6.2.*",
        "symfony/http-client-contracts": "^3.0",
        "symfony/http-client-implementation": "^3.0",
        "symfony/http-foundation": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/messenger": "^6.1",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/redis-messenger": "6.2.*",
        "symfony/runtime": "^6.1",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/slack-notifier": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/uid": "^6.1",
        "symfony/validator": "6.2.*",
        "symfony/yaml": "6.2.*",
        "thecodingmachine/safe": "^2.4",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "workerman/workerman": "^4.0"
    },
    "require-dev": {
        "blackfire/php-sdk": "^1.31",
        "doctrine/coding-standard": "^10.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/debug-bundle": "6.2.*",
        "symfony/maker-bundle": "^1.47",
        "symfony/phpunit-bridge": "6.2.*",
        "symfony/process": "^6.1",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*",
        "symplify/monorepo-builder": "^11.1",
        "theofidry/psysh-bundle": "^4.5"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "exclude-from-classmap": [
            "packages/manyou/packages/mango/Tests/",
            "packages/manyou/packages/promise-http-client/Tests/"
        ],
        "psr-4": {
            "App\\": "src/",
            "Manyou\\BingHomepage\\": "packages/manyou/packages/bing-homepage/",
            "Manyou\\ClearServiceTagsBundle\\": "packages/manyou/packages/clear-service-tags-bundle/",
            "Manyou\\LeanStorage\\": "packages/manyou/packages/leanstorage/",
            "Manyou\\Mango\\": "packages/manyou/packages/mango/",
            "Manyou\\PromiseHttpClient\\": "packages/manyou/packages/promise-http-client/",
            "Manyou\\WorkermanSymfonyRuntime\\": "packages/manyou/packages/workerman-symfony-runtime/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "manyou/bing-homepage": "self.version",
        "manyou/clear-service-tags-bundle": "self.version",
        "manyou/leanstorage": "self.version",
        "manyou/mango": "self.version",
        "manyou/promise-http-client": "self.version",
        "manyou/workerman-symfony-runtime": "self.version",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
