{
  "name": "manyou/promise-http-client",
  "description": "Make a Symfony HttpClient to return Guzzle promises",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Brent, Bohan Yang",
      "email": "brent@manyou.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/http-client-contracts": "^3.1",
    "guzzlehttp/promises": "^1.5",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/process": "^6.1",
    "symfony/http-client": "^6.1",
    "monolog/monolog": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Manyou\\PromiseHttpClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Manyou\\PromiseHttpClient\\Tests\\": "tests/"
    }
  }
}
